@import '../../_styl/vars/*.styl'

.kc-headerLogoLink, .kc-headerLogo
  display: inline-block
.kc-headerLogoLink
  display: flex
  align-items: center
  transition: 300ms ease-in-out
  &:hover
    .kc-svg-keyholeLogoSolid__icon
      fill: $white
      transform: rotate(-90deg)
    .kc-svg-keyholeLogoNew__icon
      fill: $white
      transform: rotate(-90deg)
.kc-headerLogo
  height: 40px
  display: flex
  align-items: center
  transition: 300ms ease-in-out
.kc-svg-keyholeLogoSolid
  height: 26px
.kc-svg-keyholeLogoSolid__icon
  transition: 300ms ease-out
  transform: none
  transform-origin: 919.6px 124px
.kc-svg-keyholeLogoNew
  height: 40px
.kc-svg-keyholeLogoNew__icon
  transition: 300ms cubic-bezier(0.33, -0.24, 0.21, 1.84)
  transform: none
  transform-origin: 12.75px 286.5px